  "dependencies": {
    "dotenv": "^16.4.7",
    "next": "^13.5.8",
    "pg": "8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^2.8.8"
  }
}
